{
    "name": "pankitgami/dusk",
    "description": "This is the form of laravel/dusk to support laravel 5.3",
    "keywords": ["laravel", "testing", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "facebook/webdriver": "~1.0",
        "nesbot/carbon": "~1.20",
        "illuminate/support": "~5.3",
        "symfony/process": "3.1.*",
        "symfony/console": "3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.6"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Dusk\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
