{
    "name": "pankitgami/dusk",
    "description": "This package is the fork of laravel/dusk to support dusk in laravel 5.3",
    "keywords": ["laravel", "laravel-5.3", "testing", "webdriver"],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "facebook/webdriver": "~1.0",
        "nesbot/carbon": "~1.20",
        "illuminate/support": "~5.3",
        "symfony/process": "3.1.*",
        "symfony/console": "3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.6"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Dusk\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
